# Environment Configuration Example
# Copy this file to .env.local and update with your values

# App Configuration
EXPO_PUBLIC_APP_NAME=Stuff Happens
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_ENV=development

# API Configuration (for future backend integration)
EXPO_PUBLIC_API_URL=https://api.stuffhappens.com
EXPO_PUBLIC_API_TIMEOUT=10000

# Supabase Configuration
# Development Environment (use for local development and PR previews)
EXPO_PUBLIC_SUPABASE_URL=your_dev_supabase_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_dev_supabase_anon_key_here

# Production Environment (only set these in production builds)
# EXPO_PUBLIC_SUPABASE_URL_PROD=your_prod_supabase_url_here
# EXPO_PUBLIC_SUPABASE_ANON_KEY_PROD=your_prod_supabase_anon_key_here

# Supabase Features
EXPO_PUBLIC_SUPABASE_AUTH_REDIRECT_URL=exp://localhost:19000/auth/callback
EXPO_PUBLIC_SUPABASE_REALTIME_ENABLED=true

# Analytics (optional)
EXPO_PUBLIC_ANALYTICS_ENABLED=false
EXPO_PUBLIC_ANALYTICS_KEY=your_analytics_key_here

# Debug Configuration
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_LOG_LEVEL=info